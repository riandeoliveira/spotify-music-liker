{
  "manifest_version": 2,
    "name": "Injecta",
    "version": "0.0.1",
    "description": "Injecting stuff",
    "background": {
      "scripts": [
        "background.js"
      ] 
    },
    "browser_action": {
      "default_title": "Inject!"
    },
    "permissions": [
      "https://open.spotify.com/playlist/*",
      "tabs"
    ]
  }